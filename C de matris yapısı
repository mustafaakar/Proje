#include <stdio.h>
#include <locale.h>

void raSayi(int *);

int main(){
	
	setlocale(LC_ALL, "Turkish"); // Türkçe karakterleri destekler.
	int i=0,j=0,b=0;
    int a[5][3]={0};
    for(i=0; i<5; i++){
            for(j=0; j<3; j++){
               printf("[%d].firma [%d].ayı girin==",(i+1),(j+1));
               scanf("%d",&b);
               a[i][j]=b;         
            }         
    }
    for(i=0; i<5; i++){
            for(j=0; j<3; j++){
               printf("--%d--",a[i][j]);
                       
            }  
            printf("\n");       
    }
    system("pause");
	return 0;
	
}


